header
    .logo
        | prime health
    .newAddOn
        div 
            a.hello(href='/medicat') Home
        div
            a.hello(href='/alldoc') Doctors
        div 
            a.hello(href='/predictionModel') PredictionModel
    .nav-actions
        if user
            a.login.nav_el--logout
                span.ac Log out
            a.nav__el(href='/me')
                img.nav__user-img(src=`/img/users/${user.photo}`, alt=`photo of ${user.name}`)
                span.newOne= user.name.split(' ')[0]
        else
            a.login(href="/login") 
                span.ac Log in
            a.signup(href='/signup') 
                span.ac Sign up



